extends layout/baselogin
block content
    .login-box
      .login-logo
        a(href='#')
          b Daily Programming
      // /.login-logo
      .login-box-body
        p.login-box-msg Silakan Login
        form(action='#' method='post')
          label Email
          .form-group.has-feedback
            input.form-control(type='email' autocomplete='off' placeholder='Isi Email')
            span.glyphicon.glyphicon-envelope.form-control-feedback
          label Password
          .form-group.has-feedback
            input.form-control(type='password' placeholder='Isi Password' autocomplete='off')
            span.glyphicon.glyphicon-lock.form-control-feedback
          .social-auth-links.text-center
            button.btn.btn-primary.btn-block.btn-flat(type='submit') Masuk
          a.text-center(href='../register') Apakah anda sudah registrasi ?
      // /.login-box-body
    // /.login-box





							
